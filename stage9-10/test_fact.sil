decl

integer factorial (integer n;boolean flag);

enddecl

integer factorial (integer n;boolean flag) {

decl

integer rvalue;

enddecl

begin

if (n==1) then

rvalue = 1;

else

rvalue = n * factorial (n-1,TRUE);

endif;

return rvalue;  
end

}

integer main( ){ 

decl

integer n,i ;

enddecl

begin

read (n);

i = 1;

while ( i <= n) do

write ( factorial(i,TRUE));

i = i + 1;

endwhile;

return 1;   

end

}


